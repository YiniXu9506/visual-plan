{
  "name": "visual-plan",
  "version": "0.0.1",
  "description": "A React component to show a visualization of execution plan.",
  "keywords": [
    "react",
    "component",
    "ui-component",
    "visual-plan",
    "visual plan"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "targets": {
    "main": {
      "source": "src/index.ts",
      "distDir": "dist/index.js",
      "optimize": true,
      "includeNodeModules": {
        "react": false,
        "react-dom": false,
        "d3": false
      }
    },
    "detail-drawer": {
      "source": "src/DetailDrawer.tsx",
      "distDir": "dist/detail-drawer",
      "optimize": true,
      "includeNodeModules": {
        "react": false,
        "react-dom": false,
        "d3": false
      }
    }
  },
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "build": "parcel build --no-optimize",
    "build:analyze": "parcel build --reporter @parcel/reporter-bundle-analyzer",
    "dev": "parcel watch",
    "sb": "start-storybook -p 6006",
    "build-sb": "build-storybook"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@mdx-js/react": "^2.1.2",
    "@parcel/packager-ts": "^2.6.2",
    "@parcel/reporter-bundle-analyzer": "2.6.2",
    "@parcel/transformer-less": "^2.6.2",
    "@parcel/transformer-typescript-types": "^2.6.2",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-interactions": "^6.5.9",
    "@storybook/addon-links": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/react": "^6.5.9",
    "@storybook/testing-library": "^0.0.13",
    "@types/d3": "^7.4.0",
    "babel-loader": "^8.2.5",
    "d3": "^5.16.0",
    "html-webpack-plugin": "^5.5.0",
    "less": "^3.13.1",
    "less-loader": "^11.0.0",
    "parcel": "^2.6.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.22.3",
    "d3-flextree": "^2.1.2",
    "react-json-view": "^1.21.3"
  },
  "peerDependencies": {
    "d3": "5",
    "react": "17"
  },
  "overrides": {
    "react-json-view": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    }
  }
}
